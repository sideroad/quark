<div id="user-form">
    <h1>$w{user.search}</h1>
    <form action="" id="user-search-form">
        <p>
            <input type="text" id="userSearch" name="userSearch" data-quark-event="enter-user.search:form" />
            <input value="$w{search}" type="button" data-quark-event="click-user.search:form" />
        </p>
        <div id="userSearch-validate">
        	<p>$w{user.name.validate}</p>
        </div>
    </form>
</div>