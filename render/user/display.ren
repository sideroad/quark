<div id="user-display">
	<div id="userImage" ><img src="img/users/${img}.png" alt="User Image" /></div>
	<div id="userProfile">
        <p>${name}</p>
        <p>${info}</p>
        <p><input type="button" value="$w{signin}" data-quark-event="click-auth.open" ></p>
        <p><input type="button" value="$w{message.take}" data-quark-event="click-message.take" ></p>
    </div>
</div>